\definecolor{compostelamarroon}{RGB}{0,255,0}
\definecolor{compostelagreen}{RGB}{0,255,0}
\definecolor{compostelared}{RGB}{255,0,0}
\definecolor{compostelalightgray}{RGB}{204,204,204}
